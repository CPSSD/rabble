{
  "name": "fatty",
  "version": "0.0.1",
  "description": "A fat client for Rabble.",
  "main": "index.tsx",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode development",
    "build:prod": "webpack --config webpack.config.js -d --mode production",
    "start": "webpack-dev-server --mode development --content-base ./dist --hot --inline --colors --port 3000 --open",
    "lint": "eslint src --ext ts",
    "clean": "rm dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpssd/rabble.git"
  },
  "keywords": [
    "cryptocurrency"
  ],
  "bugs": {
    "url": "https://github.com/cpssd/rabble/issues"
  },
  "homepage": "https://github.com/cpssd/rabble#readme",
  "dependencies": {
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
